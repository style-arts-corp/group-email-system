{
  "name": "hosting",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x",
    "pnpm": "9.x",
    "yarn": "Please use pnpm!",
    "npm": "Please use pnpm!"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "axios": "^1.7.3",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.446.0",
    "mui": "^0.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/encoding-japanese": "^2.2.1",
    "@types/eslint": "^8.56.10",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-jsx-a11y": "^6.9.0",
    "@types/jest": "^27.5.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "cspell": "^8.14.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-ban": "^1.6.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "3.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-inspect": "^0.8.7",
    "vite-plugin-sentry": "^1.4.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "fix": "pnpm run '/.*:fix$/'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:cache": "eslint 'src/**/*.{js,jsx,ts,tsx}' --cache --cache-strategy=content",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "fmt": "prettier --check 'src/**/*.{js,jsx,ts,tsx,html,json}'",
    "fmt:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,html,json}'",
    "typecheck": "tsc",
    "spellcheck": "cspell 'src/**/*.{js,jsx,ts,tsx,html,json}' --no-progress",
    "test": "vitest run",
    "test:typecheck": "vitest --typecheck.only",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}